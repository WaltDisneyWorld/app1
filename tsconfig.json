{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/dashboardContent/jobContent/newCopy/index.jsx",
    "store/index.ts",
    "components/admin/contents/business_api/users/cuModal",
    "components/admin/contents/website/userDetail",
    "components/admin/contents/website/userDetail.tsx/teamInfo"
  ],
  "exclude": ["node_modules"]
}
